class Solution {
    public void rotate(int[][] matrix) {
      int n = matrix.length; 
      for(int i=0; i<n; i++){
        for(int j=i; j<n; j++){
            int k = matrix[i][j];
            matrix[i][j] = matrix[j][i];
            matrix[j][i] = k;
        }
      } 
â€¦            int k = arr[start];
            arr[start] = arr[end];
            arr[end] = k;
            start++;
            end--;
        }
    }
}
